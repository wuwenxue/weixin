package co.dc.server.weixin.pojo;

import java.io.Serializable;

import co.dc.commons.utils.DateTimeUtil;

public class WeixinToSenderPojo implements Serializable{

	private static final long serialVersionUID = 1L;
	
	public static final String tableName = "wx_weixintosender";

	private long id;
	
	private String senderId;
	
	private String toUserName;
	
	private String fromUserName;
	
	private String msgType;
	
	private String content;
	
	private String createDate = DateTimeUtil.getDateTimeString();
	
	private String plat;
	
	private int delFlag = 0;

	private String useNickname;
	
	private int useSex;
	
	private String useAddress;
	
	private String useHeadimgurl;
	
	public String getPlat() {
		return plat;
	}

	public void setPlat(String plat) {
		this.plat = plat;
	}

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public String getSenderId() {
		return senderId;
	}

	public void setSenderId(String senderId) {
		this.senderId = senderId;
	}

	public String getToUserName() {
		return toUserName;
	}

	public void setToUserName(String toUserName) {
		this.toUserName = toUserName;
	}

	public String getFromUserName() {
		return fromUserName;
	}

	public void setFromUserName(String fromUserName) {
		this.fromUserName = fromUserName;
	}

	public String getMsgType() {
		return msgType;
	}

	public void setMsgType(String msgType) {
		this.msgType = msgType;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public String getCreateDate() {
		return createDate;
	}

	public void setCreateDate(String createDate) {
		this.createDate = createDate;
	}

	public int getDelFlag() {
		return delFlag;
	}

	public void setDelFlag(int delFlag) {
		this.delFlag = delFlag;
	}

	public String getUseNickname() {
		return useNickname;
	}

	public void setUseNickname(String useNickname) {
		this.useNickname = useNickname;
	}

	public int getUseSex() {
		return useSex;
	}

	public void setUseSex(int useSex) {
		this.useSex = useSex;
	}

	public String getUseAddress() {
		return useAddress;
	}

	public void setUseAddress(String useAddress) {
		this.useAddress = useAddress;
	}

	public String getUseHeadimgurl() {
		return useHeadimgurl;
	}

	public void setUseHeadimgurl(String useHeadimgurl) {
		this.useHeadimgurl = useHeadimgurl;
	}
	
	
}
