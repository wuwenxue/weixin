		<display-name>wxmo</display-name>
		<servlet-name>wxmo</servlet-name>
		<servlet-class>co.dc.server.weixin.TikiboxWeiXinServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>wxmo</servlet-name>
		<url-pattern>/wxmo</url-pattern>
	</servlet-mapping> -->

	<servlet>
		<display-name>twxmo</display-name>
		<servlet-name>twxmo</servlet-name>
		<servlet-class>co.dc.server.weixin.JiayouWeiXinServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>twxmo</servlet-name>
		<url-pattern>/twxmo</url-pattern>
	</servlet-mapping>


</web-app>